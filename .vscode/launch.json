{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Chrome",
      "request": "launch",
      "type": "pwa-chrome",
      "trace": true,
      "url": "http://localhost/~jyg/homepage",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Listen for Xdebug",
      "type": "php",
      "request": "launch",
      "port": 9000,
      "pathMappings": {
        "/home": "/Users"
      }
    }
  ]
}
